    {
      "cell_type": "code",
      "source": [
        "belanja= float(input(\"masukkan total belanja: Rp \"))\n",
        "if total>=500000:\n",
        "  diskon=0.2\n",
        "elif total>=250000:\n",
        "  diskon=0.1\n",
        "else:\n",
        "  diskon=0\n",
        "potongan= belanja*diskon\n",
        "total=belanja-potongan\n",
        "print(\"total belanja : Rp\", belanja)\n",
        "print(\"potongan      : \", (diskon*100),\"%\")\n",
        "print(\"total bayar   : Rp \", total)"
      ],
