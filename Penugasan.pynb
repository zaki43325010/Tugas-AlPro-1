      }
    },
    {
      "cell_type": "code",
      "source": [
        "total=int(input(\"masukkan waktu dalam detik: \"))\n",
        "if total<0:\n",
        "  print(\"tidak boleh negatif.\")\n",
        "else:\n",
        "  jam= total // 3600\n",
        "  sisa= total % 3600\n",
        "  menit= sisa // 60\n",
        "  detik= sisa % 60\n",
        "print (f\"{jam}:{menit}:{detik}\")\n",
        ""
      ],
